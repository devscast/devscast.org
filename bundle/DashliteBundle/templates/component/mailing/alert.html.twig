{% if this.format == 'html' %}
    <table class="panel" width="100%" cellpadding="0" cellspacing="0" role="presentation">
        <tr>
            <td class="panel-content">
                <table width="100%" cellpadding="0" cellspacing="0" role="presentation">
                    <tr>
                        <td class="panel-item">
                            {{ this.message }}
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
{% else %}
    {{ this.message }}
{% endif %}
