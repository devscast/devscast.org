<div class="nk-tb-col {{ this.responsive }} {{ this.tools ? 'nk-tb-col-tools' : '' }}">
    <twig:block name="content">
        {% if not this.col %}
            {{ this.value | raw }}
        {% else %}
            <span class="sub-text fw-bolder">{{ this.value }}</span>
        {% endif %}
    </twig:block>
</div>
