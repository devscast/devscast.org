<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="dashlite.twig.icon_extension" class="Devscast\Bundle\DashliteBundle\Extension\TwigIconExtension" public="true">
            <tag name="twig.extension" />
            <argument type="service" id="request_stack"/>
        </service>
        <service id="dashlite.twig.badge_extension" class="Devscast\Bundle\DashliteBundle\Extension\TwigBadgeExtension" public="true">
            <tag name="twig.extension" />
            <argument type="service" id="translator"/>
        </service>
    </services>
</container>
