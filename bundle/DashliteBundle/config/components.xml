<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="devscast_dashlite.twig.component.sidebar" class="DashliteSidebar" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/sidebar.html.twig"
            />
            <argument type="service" id="request_stack"/>
        </service>

        <!-- tables -->
        <service id="devscast_dashlite.twig.component.table.responsive" class="DashliteResponsiveTable" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/table/responsive.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.table.responsive.column" class="DashliteResponsiveTableColumn" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/table/responsive/column.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.table.responsive.tools" class="DashliteResponsiveTableTools" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/table/responsive/tools.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.table.responsive.action" class="DashliteResponsiveTableAction" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/table/responsive/action.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.table.bordered" class="DashliteBorderedTable" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/table/responsive.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.table.transaction" class="DashliteTransactionTable" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/table/responsive.html.twig"
            />
        </service>

        <!-- cards -->
        <service id="devscast_dashlite.twig.component.card.modal" class="DashliteModal" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/card/modal.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.card.empty_state" class="DashliteEmptyStateCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/card/empty_state.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.card.card" class="DashliteCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/card/card.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.card.stretch" class="DashliteStretchCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/card/stretch.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.card.overview" class="DashliteOverviewCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/card/overview.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.card.aside" class="DashliteAsideCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/card/aside.html.twig"
            />
        </service>

        <!-- data -->
        <service id="devscast_dashlite.twig.component.data.list" class="DashliteDataList" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/data/list.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.data.item" class="DashliteDataItem" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/data/item.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.data.meta_item" class="DashliteMetaDataItem" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/data/meta_item.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.data.meta_section" class="DashliteMetaDataSection" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/data/meta_section.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.data.meta_list" class="DashliteMetaDataList" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/data/meta_list.html.twig"
            />
        </service>

        <!-- button -->
        <service id="devscast_dashlite.twig.component.button.delete" class="DashliteDeleteButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/button/delete.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.button.modal" class="DashliteFormModalButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/button/form_modal.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.button.link" class="DashliteLinkButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/button/link.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.button.header_link" class="DashliteHeaderLinkButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/button/link.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.button.user_avatar_link" class="DashliteUserAvatarLink" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/button/user_avatar_link.html.twig"
            />
        </service>

        <!-- layout -->
        <service id="devscast_dashlite.twig.component.layout.header" class="DashliteHeader" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/layout/header.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.layout.footer" class="DashliteFooter" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/layout/footer.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.layout.navbar" class="DashliteNavbar" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/layout/navbar.html.twig"
            />
        </service>

        <!-- generic -->
        <service id="devscast_dashlite.twig.component.flash" class="DashliteFlashMessage" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/flash.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.form" class="DashliteForm" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/form.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.note" class="DashliteNote" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/form.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.icon" class="DashliteIcon" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/icon.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.alert" class="DashliteAlert" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/alert.html.twig"
            />
        </service>

        <!-- mailing -->
        <service id="devscast_dashlite.twig.component.mailing.button" class="DashliteEmailButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/mailing/button.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.mailing.alert" class="DashliteEmailAlert" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/mailing/alert.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.mailing.section" class="DashliteEmailSection" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/mailing/section.html.twig"
            />
        </service>
        <service id="devscast_dashlite.twig.component.mailing.qrcode" class="Devscast\Bundle\DashliteBundle\Component\Mailing\EmailQrCode" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/component/mailing/qrcode.html.twig"
            />
        </service>
    </services>
</container>
