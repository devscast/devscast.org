<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="dashlite.twig.component.sidebar" class="Devscast\Bundle\DashliteBundle\Component\Sidebar\Sidebar" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/sidebar.html.twig"
            />
            <argument type="service" id="request_stack"/>
        </service>

        <!-- tables -->
        <service id="dashlite.twig.component.table.responsive" class="Devscast\Bundle\DashliteBundle\Component\Table\ResponsiveTable" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/table/responsive.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.table.responsive.column" class="Devscast\Bundle\DashliteBundle\Component\Table\Responsive\ResponsiveTableColumn" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/table/responsive/column.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.table.responsive.tools" class="Devscast\Bundle\DashliteBundle\Component\Table\Responsive\ResponsiveTableTools" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/table/responsive/tools.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.table.responsive.action" class="Devscast\Bundle\DashliteBundle\Component\Table\Responsive\ResponsiveTableAction" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/table/responsive/action.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.table.bordered" class="Devscast\Bundle\DashliteBundle\Component\Table\BorderedTable" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/table/responsive.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.table.transaction" class="Devscast\Bundle\DashliteBundle\Component\Table\TransactionTable" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/table/responsive.html.twig"
            />
        </service>

        <!-- cards -->
        <service id="dashlite.twig.component.card.modal" class="Devscast\Bundle\DashliteBundle\Component\Card\Modal" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/card/modal.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.card.empty_state" class="Devscast\Bundle\DashliteBundle\Component\Card\EmptyStateCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/card/empty_state.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.card.card" class="Devscast\Bundle\DashliteBundle\Component\Card\Card" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/card/card.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.card.stretch" class="Devscast\Bundle\DashliteBundle\Component\Card\StretchCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/card/stretch.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.card.overview" class="Devscast\Bundle\DashliteBundle\Component\Card\OverviewCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/card/overview.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.card.aside" class="Devscast\Bundle\DashliteBundle\Component\Card\AsideCard" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/card/aside.html.twig"
            />
        </service>

        <!-- data -->
        <service id="dashlite.twig.component.data.list" class="Devscast\Bundle\DashliteBundle\Component\Data\DataList" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/data/list.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.data.item" class="Devscast\Bundle\DashliteBundle\Component\Data\DataItem" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/data/item.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.data.meta_item" class="Devscast\Bundle\DashliteBundle\Component\Data\MetaDataItem" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/data/meta_item.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.data.meta_section" class="Devscast\Bundle\DashliteBundle\Component\Data\MetaDataSection" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/data/meta_section.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.data.meta_list" class="Devscast\Bundle\DashliteBundle\Component\Data\MetaDataList" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/data/meta_list.html.twig"
            />
        </service>

        <!-- button -->
        <service id="dashlite.twig.component.button.delete" class="Devscast\Bundle\DashliteBundle\Component\Button\DeleteButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/button/delete.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.button.modal" class="Devscast\Bundle\DashliteBundle\Component\Button\FormModalButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/button/form_modal.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.button.link" class="Devscast\Bundle\DashliteBundle\Component\Button\LinkButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/button/link.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.button.header_link" class="Devscast\Bundle\DashliteBundle\Component\Button\HeaderLinkButton" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/button/link.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.button.user_avatar_link" class="Devscast\Bundle\DashliteBundle\Component\Button\UserAvatarLink" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/button/user_avatar_link.html.twig"
            />
        </service>

        <!-- layout -->
        <service id="dashlite.twig.component.layout.header" class="Devscast\Bundle\DashliteBundle\Component\Layout\Header" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/layout/header.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.layout.footer" class="Devscast\Bundle\DashliteBundle\Component\Layout\Footer" public="true">
            <tag
                name="twig.component"
