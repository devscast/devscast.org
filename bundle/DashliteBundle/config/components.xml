            />
        </service>
        <service id="dashlite.twig.component.layout.navbar" class="Devscast\Bundle\DashliteBundle\Component\Layout\Navbar" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/layout/navbar.html.twig"
            />
        </service>

        <!-- generic -->
        <service id="dashlite.twig.component.flash" class="Devscast\Bundle\DashliteBundle\Component\FlashMessage" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/flash.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.form" class="Devscast\Bundle\DashliteBundle\Component\Form" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/form.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.note" class="Devscast\Bundle\DashliteBundle\Component\Note" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/form.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.icon" class="Devscast\Bundle\DashliteBundle\Component\Icon" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/icon.html.twig"
            />
        </service>
        <service id="dashlite.twig.component.alert" class="Devscast\Bundle\DashliteBundle\Component\Alert" public="true">
            <tag
                name="twig.component"
                template="@DevscastDashlite/_component/alert.html.twig"
            />
        </service>
    </services>
</container>
